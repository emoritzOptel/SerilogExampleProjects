{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Elasticsearch"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "",
          "indexFormat": "example.unit.test.{0:yyyy.MM}",
          "autoRegisterTemplate": "true",
          "autoRegisterTemplateVersion": "ESv6"
        }
      }
    ]
  }
}